"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"29"},"C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\index.js",[],[],[],"C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\App.js",[],[],"C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\pages\\SearchBooks.js",["65","66"],[],"C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\pages\\SavedBooks.js",["67","68","69"],[],"C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\components\\Navbar.js",[],[],"C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\components\\LoginForm.js",["70","71"],[],"// see SignupForm.js for comments\nimport React, { useState } from 'react';\nimport { Form, Button, Alert } from 'react-bootstrap';\nimport { useMutation } from '@apollo/client';\nimport { LOGIN_USER } from '../utils/mutations';\nimport Auth from '../utils/auth';\n\nconst LoginForm = () => {\n  const [userFormData, setUserFormData] = useState({ email: '', password: '' });\n  const [validated] = useState(false);\n  const [showAlert, setShowAlert] = useState(false);\n  const [login, { error, data }] = useMutation(LOGIN_USER);\n\n  const handleInputChange = (event) => {\n    const { name, value } = event.target;\n    setUserFormData({ ...userFormData, [name]: value });\n  };\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    // check if form has everything (as per react-bootstrap docs)\n    const form = event.currentTarget;\n    if (form.checkValidity() === false) {\n      event.preventDefault();\n      event.stopPropagation();\n    }\n\n    try {\n      const { data } = await login({\n        variables: { ...userFormData }, \n      });\n\n      Auth.login(data.login.token);\n    } catch (err) {\n      console.error(err);\n      setShowAlert(true);\n    }\n\n    setUserFormData({\n      username: '',\n      email: '',\n      password: '',\n    });\n  };\n\n  return (\n    <>\n    \n      <Form noValidate validated={validated} onSubmit={handleFormSubmit}>\n        <Alert dismissible onClose={() => setShowAlert(false)} show={showAlert} variant='danger'>\n          Something went wrong with your login credentials!\n        </Alert>\n        <Form.Group className='mb-3'>\n          <Form.Label htmlFor='email'>Email</Form.Label>\n          <Form.Control\n            type='text'\n            placeholder='Your email'\n            name='email'\n            onChange={handleInputChange}\n            value={userFormData.email}\n            required\n          />\n          <Form.Control.Feedback type='invalid'>Email is required!</Form.Control.Feedback>\n        </Form.Group>\n\n        <Form.Group className='mb-3'>\n          <Form.Label htmlFor='password'>Password</Form.Label>\n          <Form.Control\n            type='password'\n            placeholder='Your password'\n            name='password'\n            onChange={handleInputChange}\n            value={userFormData.password}\n            required\n          />\n          <Form.Control.Feedback type='invalid'>Password is required!</Form.Control.Feedback>\n        </Form.Group>\n        <Button\n          disabled={!(userFormData.email && userFormData.password)}\n          type='submit'\n          variant='success'>\n          Submit\n        </Button>\n      </Form>\n    </>\n  );\n};\n\nexport default LoginForm;\n","C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\utils\\auth.js",["72"],[],"// use this to decode a token and get the user's information out of it\nimport decode from 'jwt-decode';\n\n// create a new class to instantiate for a user\nclass AuthService {\n  // get user data\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  // check if user's logged in\n  loggedIn() {\n    // Checks if there is a saved token and it's still valid\n    const token = this.getToken();\n    return token && !this.isTokenExpired(token); // handwaiving here\n  }\n\n  // check if token is expired\n  isTokenExpired(token) {\n    try {\n      const decoded = decode(token);\n      if (decoded.exp < Date.now() / 1000) {\n        return true;\n      } else return false;\n    } catch (err) {\n      return false;\n    }\n  }\n\n  getToken() {\n    // Retrieves the user token from localStorage\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    // Saves user token to localStorage\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();\n","C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\components\\SignupForm.js",["73","74"],[],"C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\utils\\queries.js",[],[],"C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\utils\\API.js",[],[],"C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\utils\\mutations.js",[],[],"C:\\Users\\dahne\\bootcamp\\challenges\\books-r-babes\\client\\src\\utils\\localStorage.js",[],[],{"ruleId":"75","severity":1,"message":"76","line":24,"column":22,"nodeType":"77","messageId":"78","endLine":24,"endColumn":27},{"ruleId":"75","severity":1,"message":"79","line":77,"column":15,"nodeType":"77","messageId":"78","endLine":77,"endColumn":19},{"ruleId":"75","severity":1,"message":"80","line":16,"column":11,"nodeType":"77","messageId":"78","endLine":16,"endColumn":18},{"ruleId":"75","severity":1,"message":"76","line":17,"column":24,"nodeType":"77","messageId":"78","endLine":17,"endColumn":29},{"ruleId":"75","severity":1,"message":"79","line":27,"column":15,"nodeType":"77","messageId":"78","endLine":27,"endColumn":19},{"ruleId":"75","severity":1,"message":"76","line":12,"column":19,"nodeType":"77","